<%
for (int t = 1; t <= 10; t++) {
out.println("Tabuada do: " + t + "<br>");
for (int n = 1; n <= 10; n++) {
out.println(t + " x " + n + " = " + (t * n) + "<br>");
  }
out.println("<hr>");
  }
 %>
